<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/conts_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#90050101"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <ImageView
        android:id="@+id/img_popup"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:background="@drawable/bg_button_login"
        android:backgroundTint="#F0F0F0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription"/>

    <TextView
        android:id="@+id/tv_estimated_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예상 제조 시간"
        android:layout_marginTop="12dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/inter_regular"
        android:textColor="@color/black"
        app:layout_constraintTop_toTopOf="@id/img_popup"
        app:layout_constraintStart_toStartOf="@id/img_popup"/>

    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:background="#A8A8A8"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@id/tv_estimated_time"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup"/>

    <TextView
        android:id="@+id/img_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="X"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="9dp"
        app:layout_constraintTop_toTopOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup"/>

    <TextView
        android:id="@+id/tv_5_minute"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginBottom="15dp"
        android:text="5분"
        android:textSize="14sp"
        android:textColor="@color/white"
        android:gravity="center"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/inter_regular"
        android:background="@drawable/bg_button_login"
        app:layout_constraintTop_toBottomOf="@id/view"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    <TextView
        android:id="@+id/tv_10_minute"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginBottom="15dp"
        android:text="10분"
        android:textSize="14sp"
        android:textColor="@color/white"
        android:gravity="center"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/inter_regular"
        android:background="@drawable/bg_button_login"
        app:layout_constraintTop_toBottomOf="@id/tv_5_minute"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    <TextView
        android:id="@+id/tv_15_minute"
        android:layout_width="0dp"
        android:layout_height="55dp"
        android:layout_marginBottom="15dp"
        android:text="15분"
        android:textSize="14sp"
        android:textColor="@color/white"
        android:gravity="center"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:fontFamily="@font/inter_regular"
        android:background="@drawable/bg_button_login"
        app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup"/>

    <LinearLayout
        android:id="@+id/linear_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:weightSum="2"
        app:layout_constraintTop_toBottomOf="@id/tv_15_minute"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup">

        <TextView
            android:id="@+id/tv_20_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="20분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
        <TextView
            android:id="@+id/tv_25_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="25분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        app:layout_constraintTop_toBottomOf="@id/linear_1"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup">

        <TextView
            android:id="@+id/tv_30_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="30분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
        <TextView
            android:id="@+id/tv_35_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="35분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        app:layout_constraintTop_toBottomOf="@id/linear_2"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup">

        <TextView
            android:id="@+id/tv_40_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="40분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
        <TextView
            android:id="@+id/tv_45_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="45분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2"
        app:layout_constraintTop_toBottomOf="@id/linear_3"
        app:layout_constraintStart_toStartOf="@id/img_popup"
        app:layout_constraintEnd_toEndOf="@id/img_popup">

        <TextView
            android:id="@+id/tv_50_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="50분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
        <TextView
            android:id="@+id/tv_55_minute"
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:text="55분"
            android:layout_weight="1"
            android:textSize="14sp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginTop="9dp"
            android:layout_marginEnd="15dp"
            android:layout_marginStart="15dp"
            android:fontFamily="@font/inter_regular"
            android:background="@drawable/bg_button_login"
            app:layout_constraintTop_toBottomOf="@id/tv_10_minute"
            app:layout_constraintStart_toStartOf="@id/img_popup"
            app:layout_constraintEnd_toEndOf="@id/img_popup"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
